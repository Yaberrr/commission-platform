<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moe.admin.mapper.UserMessageMapper">


    <select id="selectMessageByUserId" resultType="com.moe.admin.domain.vo.user.MessageVO">
        select id, title, content, create_time, status
        from rb_user_message
        where user_id = #{userId}
    </select>

</mapper>